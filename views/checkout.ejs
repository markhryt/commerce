<html>
    <head>
        <title>
            checkout
        </title>
    </head>
    <body>
        <h1>Checkout</h1>
        <% cart.forEach(function(cartItem) { %>
            <div>
                  <h3><%= cartItem.name%></h3>
              </div>
           <% }); %>

        <form method="post" action="/buy">
            <input type="hidden" name="cart" value="<%= cart%>">
             <button type="submit">Buy</button>
        </form>
    </body>
</html>